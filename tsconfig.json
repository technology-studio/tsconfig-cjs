{
  "compilerOptions": {
    "lib": ["esnext"],
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noEmit": true,
    "declaration": false,
    "esModuleInterop": true,
    "alwaysStrict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "sourceMap": true,
    "target": "ES2020",
    "moduleResolution": "Node",
    "module": "CommonJS",
    "resolveJsonModule": true
  }
}
